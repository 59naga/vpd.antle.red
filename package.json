{
  "name": "vpd.antle.red",
  "private": true,
  "main": "server.coffee",
  "scripts": {
    "postinstall": "bower install && npm run build",
    "build": "NODE_ENV=production npm-run-all --parallel build:*",
    "build:bower": "onefile -o dist/pkgs.min.js -m",
    "build:coffee": "browserify index.coffee --extension .coffee -t coffeeify -t [browserify-ngannotate --ext .coffee] | uglifyjs -cm > dist/index.js",
    "build:jade": "jade index.jade --out dist",
    "build:stylus": "stylus --use kouto-swiss --import kouto-swiss --compress index.styl --out dist",
    "start": "coffee server.coffee"
  },
  "dependencies": {
    "bluebird": "^2.10.2",
    "browserify": "^12.0.1",
    "browserify-ngannotate": "^1.0.1",
    "coffeeify": "^1.1.0",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "kouto-swiss": "^0.11.13",
    "npm-run-all": "^1.2.12",
    "onefile": "^0.3.3",
    "pako": "^0.2.8",
    "stylus": "^0.52.4",
    "vpvp-vpd": "0.0.0-beta"
  },
  "devDependencies": {
    "difficult-http-server": "^0.0.12"
  },
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/vpd.berabou.me.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/vpd.berabou.me/issues"
  },
  "homepage": "https://github.com/59naga/vpd.berabou.me#readme"
}
